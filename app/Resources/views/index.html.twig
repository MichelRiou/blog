{% extends 'base.html.twig' %}

{% block body %}

<h2>Liste des dernieres Demandes/recherches</h2>
    <div class="col-md-9 col-md-offset-3">
    <table class="table table-bordered table-striped">
    {% for post in posts %}
    <tr><td>{{ post.createdAt | date('d/m/Y') }}</td><td>{{ post.type.designation }}</td><td>{{ post.titre }}</td><td>{{ post.member.name }}</td></tr>

        {% endfor %}

    </table>
    </div>

{% endblock %}